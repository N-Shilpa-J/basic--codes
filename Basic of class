//Basic concepts of Class definition


#include <iostream>
using namespace std;
class SE
{
    public:
    char name[20];
    int roll_no;
    public:
    void get_data();
    void display();
};

void SE::get_data()
{
    cout<<"Enter your name"<<endl;
    cin>>name;
    cout<<"Enter your Roll no"<<endl;
    cin>>roll_no;
}

void SE::display()
{
    cout<<"Name of student is"<< name<<endl;
    cout<<"Roll no of student is"<< roll_no<<endl;
}

int main()
{
    SE a1, b1;
    a1.get_data();
    a1.display();
    b1.get_data();
    b1.display();
    return 0;
}
