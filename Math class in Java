import java.util.*;
import java.lang.Math;

public class Main
{
	public static void main(String[] args) {
	    int x;
	    double y;
	    System.out.println("Enter the number whose square root to be calculated");
	    Scanner s1=new Scanner(System.in);
	    x= s1.nextInt();
	    y= Math.sqrt(x);
	     System.out.println("Square root is: " +y);
	}
}
